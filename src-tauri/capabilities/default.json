{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "platforms": ["windows"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "websocket:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [{ "url": "https://example.com" }]
    }
  ]
}